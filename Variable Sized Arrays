#include <iostream>
using namespace std;

int main() 
{
    
    int n,q,k,s=0,m,l;
    cin>>n>>q;
    int *arr[n];
    while(n--)
    {
        cin>>k;
        arr[s]=new int[k];
        for(int i=0;i<k;i++)
        {
        cin>>arr[s][i];
        }
    
        s++;
    }
        
    while(q--)
    {
     
        cin>>m>>n;
        cout<<arr[m][n]<<endl;
    }
    
    return 0;
}
